@using Modum.Models.BaseModels.Models.BaseStructure;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Cache-Control" content="no-store" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <link rel="shortcut icon" type="x-icon" href="~/clothes-shop/images/logos/modum-transparent2-removebg-preview.png" />
    <title>@ViewData["Title"] - Modum</title>
    <!--=====================================
              CSS LINK PART START
      =======================================-->
    <link rel="stylesheet" type="text/css" href="~/css/custom/AllViews.css">
    <!-- FAVICON -->
    <!-- FONTS -->
    <link rel="stylesheet" href="~/clothes-shop/fonts/flaticon/flaticon.css">
    <script type="text/javascript" src="https://kit.fontawesome.com/47edf10d0e.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- VENDOR -->
    <link rel="stylesheet" href="~/clothes-shop/css/vendor/bootstrap.min.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.13/css/select2.min.css">

    <link rel="stylesheet" href="https://code.jquery.com/ui/1.13.0/themes/base/jquery-ui.css">

    <!--ALERTS-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.18/dist/sweetalert2.min.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />

    <!--CUSTOM-->
    <link rel="stylesheet" type="text/css" href="~/css/custom/GenderCallTemplate.css">
    <link rel="stylesheet" type="text/css" href="~/css/custom/CustomLayout.css">
    @RenderSection("Styles", false)
    <!--=====================================
                CSS LINK PART END
    =======================================-->
    @*name: Modum*@
    @*slogan: induite vos bene*@


</head>
<body>
    @using System.Reflection
    <header>
        <div id="head-section pb-5">
            <div class="desktop-menu">
                <nav class="navbar navbar-expand-lg navbar-toggleable-sm navbar-light box-shadow " id="header-bar">
                    <div class="container-fluid d-flex justify-content-center align-items-center">
                        <div class="d-sm-inline-flex justify-content-between">
                            <ul class=" navbar-nav flex-grow-1 justify-content-center align-items-center">
                                <li class="nav-item d-none d-sm-block ">
                                    <a class="nav-link nav-link-custom gender-section" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Women">Women</a>
                                </li>
                                <li class="nav-item d-none d-sm-block ">
                                    <a class="nav-link nav-link-custom gender-section" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Men">Men</a>
                                </li>
                                <li class="nav-item d-none d-sm-block ">
                                    <a class="nav-link nav-link-custom gender-section" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Girls">Girls</a>
                                </li>
                                <li class="nav-item d-none d-sm-block ">
                                    <a class="nav-link nav-link-custom gender-section" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Boys">Boys</a>
                                </li>
                            </ul>
                            <ul class="navbar-nav flex-grow-1 justify-content-center align-items-center">
                                <li class="nav-item nav-item-image">
                                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Women">
                                        <img src="~/clothes-shop/images/logos/modum-transparent2-removebg-preview.png" />
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <div class="input-group">
                                        <input class="form-control me-2 border-0 border-bottom border-dark" type="search" id="search-input" placeholder="Search" aria-label="Search" value=@ViewBag.SearchStringContainer />
                                        <a class="btn btn-outline-dark border-0 text-dark" id="search-link">
                                            <i class="fas fa-search"></i>
                                        </a>
                                    </div>

                                </li>
                                <!-- Favourites -->
                                <li class="nav-item">
                                    <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Home" asp-action="Favourites">
                                        <i class="far fa-heart fa-2xl"></i> <!-- Font Awesome heart icon -->
                                    </a>
                                </li>
                                <!-- Cart -->
                                <li class="nav-item">
                                    <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Home" asp-action="Cart">
                                        <i class="fas fa-shopping-cart fa-2xl"></i> <!-- Font Awesome shopping cart icon -->
                                    </a>
                                </li>
                                <!-- User -->
                                @if (User.Identity != null && User.Identity.IsAuthenticated)
                                {
                                    <li class="nav-item">
                                        <a class="nav-link nav-item fontawesome-icons" asp-area="Identity" asp-page="/Account/Manage/Index">
                                            <i class="fas fa-user fa-2xl"></i>
                                        </a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link nav-item fontawesome-icons" asp-area="Identity" asp-page="/Account/Register">
                                            <i class="far fa-user fa-2xl"></i>
                                        </a>
                                    </li>
                                }

                                @if (User.Identity != null && User.Identity.IsAuthenticated && User.IsInRole("Admin") || User.IsInRole("SuperAdmin"))
                                {
                                    <li class="nav-item">
                                        <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Admin" asp-action="ManageUsers">
                                            <i class="fa-solid fa-lock fa-2xl"></i>
                                        </a>
                                    </li>
                                }
                                @if (User.Identity != null && User.Identity.IsAuthenticated && User.IsInRole("Admin") || User.IsInRole("Worker") || User.IsInRole("SuperAdmin"))
                                {
                                    <li class="nav-item">
                                        <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Worker" asp-action="ManageProducts">
                                            <i class="fa-solid fa-screwdriver-wrench fa-2xl"></i>
                                        </a>
                                    </li>
                                }
                                <!-- End User -->
                            </ul>
                        </div>
                    </div>
                </nav>
                <div id="undersection" class="d-flex justify-content-center align-items-center">
                    @if (Model != null)
                    {
                        @if (Model.GetType().GetProperty("UnderNavCategories") != null)
                        {
                            IEnumerable<Category> categoryList = Model.UnderNavCategories;
                            IEnumerable<Brands> basicBrandList = Model.BasicBrands;
                            IEnumerable<Brands> premiumBrandList = Model.PremiumBrands;
                            @foreach (var category in categoryList)
                            {
                                <div class="item">
                                    @category.Name
                                    <div class="hover-container">
                                        <div class="d-flex justify-content-center align-items-center">
                                            <div class="col-1">
                                                <h4 class="row pb-1 subheaders">Categories</h4>
                                                @foreach (var subcategory in category.Subcategories)
                                                {
                                                    string hrefAttr = $"/Home/_UserProductsPartial?filter.MainCategoryId={category.MainCategoryId}&filter.CategoryId={category.Id}&filter.SelectedSubcategories={subcategory.Id}";
                                                    <div class="row custom-subsection-desktop-item"><i><a href=@hrefAttr>@subcategory.Name</a></i></div>
                                                }
                                            </div>
                                            @if (basicBrandList != null)
                                            {
                                                <div class="col-1">
                                                    <h4 class="row pb-1 subheaders">Brands</h4>
                                                    @foreach (var brand in basicBrandList)
                                                    {
                                                        string hrefAttr = $"/Home/_UserProductsPartial?filter.Brands={brand.BrandName}";
                                                        <div class="row custom-subsection-desktop-item"><i><a href=@hrefAttr>@brand.BrandName</a></i></div>
                                                    }
                                                </div>
                                            }
                                            @if (premiumBrandList != null)
                                            {
                                                <div class="col-1">
                                                    <h4 class="row pb-1 subheaders">Brands</h4>
                                                    @foreach (var brand in premiumBrandList)
                                                    {
                                                        string hrefAttr = $"/Home/_UserProductsPartial?filter.Brands={brand.BrandName}";
                                                        <div class="row custom-subsection-desktop-item"><i><a href=@hrefAttr>@brand.BrandName</a></i></div>
                                                    }
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                    }
                </div>
            </div>
            <div class="mobile-menu">
                <nav class="navbar navbar-expand-lg navbar-toggleable-sm navbar-light border-bottom box-shadow " id="header-bar">
                    <div class="container-fluid">
                        <div class="row pl-3">

                            <a class="btn" id="hamburger-menu">
                                <span class="navbar-toggler-icon"></span>
                            </a>
                            <div class="pl-3">
                                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Women">
                                    <img src="~/clothes-shop/images/logos/modum-transparent2.png" style="width:4em" />
                                </a>
                            </div>
                        </div>
                        <div class="row pr-4">
                            <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Home" asp-action="Favourites">
                                <i class="far fa-heart fa-2xl"></i> <!-- Font Awesome heart icon -->
                            </a>
                            <a class="nav-link nav-item fontawesome-icons" asp-area="" asp-controller="Home" asp-action="Cart">
                                <i class="fas fa-shopping-cart fa-2xl"></i> <!-- Font Awesome shopping cart icon -->
                            </a>

                            @if (User.Identity != null && User.Identity.IsAuthenticated)
                            {
                                <a class="nav-link nav-item fontawesome-icons" asp-area="Identity" asp-page="/Account/Manage/Index">
                                    <i class="fas fa-user fa-2xl"></i>
                                </a>
                            }
                            else
                            {
                                <a class="nav-link nav-item fontawesome-icons" asp-area="Identity" asp-page="/Account/Register">
                                    <i class="far fa-user fa-2xl"></i>
                                </a>
                            }
                        </div>
                        <div class="hamburger-menu" id="hamburger-menu-popup">
                            <span class="close d-flex row pr-4" id="closePopupHamburgerMenu">&times;</span>

                            <div class="hamburger-content">
                                <ul class="align-items-center justify-content-center d-flex row">
                                    <li class="nav-item m-0">
                                        <a class="nav-link nav-link-custom-mobile gender-section h4" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Women">Women</a>
                                    </li>
                                    <li class="nav-item m-0">
                                        <a class="nav-link nav-link-custom-mobile gender-section h4" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Men">Men</a>
                                    </li>
                                    <li class="nav-item m-0">
                                        <a class="nav-link nav-link-custom-mobile gender-section h4" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Girls">Girls</a>
                                    </li>
                                    <li class="nav-item m-0">
                                        <a class="nav-link nav-link-custom-mobile gender-section h4" asp-area="" asp-controller="Home" asp-action="GenderCallTemplate" asp-route-category="Boys">Boys</a>
                                    </li>
                                </ul>
                                <hr />
                                <div id="accordion" class="">
                                    @if (Model != null)
                                    {
                                        @if (Model.GetType().GetProperty("UnderNavCategories") != null)
                                        {
                                            IEnumerable<Category> categoryList = Model.UnderNavCategories;
                                            IEnumerable<Brands> basicBrandList = Model.BasicBrands;
                                            IEnumerable<Brands> premiumBrandList = Model.PremiumBrands;
                                            @foreach (var category in categoryList)
                                            {
                                                string headingId = $"heading-{category.Name}";
                                                string collapseId = $"collapse-{category.Name}";

                                                <div class="card">
                                                    @if (category.Subcategories.Count() > 0)
                                                    {
                                                        <a class="btn btn-link text-dark collapsed subcategories-list" data-toggle="collapse" data-target="#@collapseId" aria-expanded="false" aria-controls="@collapseId">
                                                            <div class="row card-header" id="@headingId">
                                                                <i class='fa-solid fa-chevron-down rotate mr-2 ml-4 down' id="collapse-color-icon"></i>
                                                                <h5 class="mb-0 pl-3">
                                                                    @category.Name
                                                                </h5>
                                                            </div>
                                                        </a>

                                                        <div id="@collapseId" class="subcategories-collapse collapse" aria-labelledby="@headingId" data-parent="#accordion">
                                                            @foreach (var subcategory in category.Subcategories)
                                                            {
                                                                string hrefAttr = $"/Home/_UserProductsPartial?filter.MainCategoryId={category.MainCategoryId}&filter.CategoryId={category.Id}&filter.SelectedSubcategories={subcategory.Id}";
                                                                <div class="row">
                                                                    <a class="btn text-dark btn-link" href="@hrefAttr">
                                                                        <div class="pl-5 row text-dark">
                                                                            <div>@subcategory.Name</div>
                                                                        </div>
                                                                    </a>
                                                                </div>
                                                            }
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        string hrefAttr = $"/Home/_UserProductsPartial?filter.MainCategoryId={category.MainCategoryId}&filter.CategoryId={category.Id}";
                                                        <a class="btn text-dark btn-link" href="@hrefAttr">
                                                            <div class="row card-header">
                                                                <h5 class="mb-0">
                                                                    @category.Name
                                                                </h5>
                                                            </div>
                                                        </a>
                                                    }


                                                </div>
                                            }


                                            @if (basicBrandList != null)
                                            {
                                                @foreach (var basicBrand in basicBrandList)
                                                {
                                                    string headingId = $"heading-{basicBrand.BrandName}";
                                                    string collapseId = $"collapse-{basicBrand.BrandName}";

                                                    <div class="card">
                                                        @{
                                                            string hrefAttr = $"/Home/_UserProductsPartial?filter.Brands={basicBrand.BrandName}";
                                                        }
                                                        <a class="btn text-dark btn-link" href="@hrefAttr">
                                                            <div class="row card-header">
                                                                <h5 class="mb-0">
                                                                    @basicBrand.BrandName
                                                                </h5>
                                                            </div>
                                                        </a>
                                                    </div>
                                                }
                                            }

                                            @if (premiumBrandList != null)
                                            {
                                                @foreach (var premiumBrand in premiumBrandList)
                                                {
                                                    string headingId = $"heading-{premiumBrand.BrandName}";
                                                    string collapseId = $"collapse-{premiumBrand.BrandName}";

                                                    <div class="card">
                                                        @{
                                                            string hrefAttr = $"/Home/_UserProductsPartial?filter.Brands={premiumBrand.BrandName}";
                                                        }
                                                        <a class="btn text-dark" href="@hrefAttr">
                                                            <div class="row card-header">
                                                                <h5 class="mb-0">
                                                                    @premiumBrand.BrandName
                                                                </h5>
                                                            </div>
                                                        </a>
                                                    </div>
                                                }
                                            }
                                        }
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </header>
    <div id="cookie-consent-message">
        <partial name="_CookieConsentPartial" />
    </div>

    <div class=".mt-3 main-selector pb-5">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <div class="row">
        <script>
            (function (w, d, s, o, f, js, fjs) {
                w["botsonic_widget"] = o;
                w[o] =
                    w[o] ||
